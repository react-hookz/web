{
	"compilerOptions": {
		"noEmit": true,
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"jsx": "react-jsx",
		"lib": [
			"ESNext",
			"DOM"
		],
		"declaration": true,
		"newLine": "lf",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
