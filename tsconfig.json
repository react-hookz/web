{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "noEmit": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
